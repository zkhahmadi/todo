{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "module": "es2015",
   "types": [
      "node"
    ],
    "paths": {
      "@src/*": [
        "src/*.web",
        "src/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts",
    "node_modules/nativescript-couchbaselite/commons.ts"
  ],
  "include": [
    ".",
    "../node_modules/@wtw/**/*"
  ],
}

